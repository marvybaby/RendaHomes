{
  "name": "rendahomes-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "compile-contracts": "node scripts/compile.js",
    "compile-all": "node scripts/compile-all.js",
    "deploy-testnet": "cross-env HEDERA_NETWORK=testnet node scripts/deploy.js",
    "deploy-mainnet": "cross-env HEDERA_NETWORK=mainnet node scripts/deploy.js",
    "deploy-all-testnet": "cross-env HEDERA_NETWORK=testnet node scripts/deploy-all.js",
    "deploy-all-mainnet": "cross-env HEDERA_NETWORK=mainnet node scripts/deploy-all.js",
    "hardhat:compile": "npx hardhat compile",
    "hardhat:deploy-hrm": "npx hardhat run hardhat-scripts/deploy-hrm.js --network hedera_testnet",
    "hardhat:deploy-all": "npx hardhat run hardhat-scripts/deploy-all.js --network hedera_testnet",
    "populate-properties": "cross-env HEDERA_NETWORK=testnet node scripts/populate-properties.js",
    "populate-defaults": "cross-env HEDERA_NETWORK=testnet node scripts/populate-default-properties.js",
    "populate-realistic": "cross-env HEDERA_NETWORK=testnet node scripts/populate-realistic-properties.js",
    "populate-governance": "cross-env HEDERA_NETWORK=testnet node scripts/populate-governance.js",
    "populate-disasters": "cross-env HEDERA_NETWORK=testnet node scripts/populate-disaster-management.js",
    "populate-all": "npm run populate-defaults && npm run populate-governance && npm run populate-disasters",
    "clean-contracts": "node scripts/compile-all.js clean",
    "verify-contracts": "node scripts/compile.js verify"
  },
  "dependencies": {
    "@hashgraph/proto": "^2.22.0",
    "@hashgraph/sdk": "^2.72.0",
    "@openzeppelin/contracts": "^4.9.6",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "hashconnect": "^3.0.13",
    "ipfs-http-client": "^60.0.1",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@types/node": "^20.19.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9",
    "hardhat": "^2.26.3",
    "postcss": "^8.5.6",
    "solc": "^0.8.30",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "util": "^0.12.5",
    "vite": "^7.1.5",
    "vite-plugin-node-polyfills": "^0.24.0"
  }
}
